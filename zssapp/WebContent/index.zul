							<!--  target="_blank" href="print.zul"/>-->
						<toolbarbutton id="undo" sclass="toolIcon" src="/img/undo.gif"
							forward="onClick=onEditUndo" />
						<toolbarbutton id="redo" sclass="toolIcon" src="/img/redo.gif"
							forward="onClick=onEditRedo" />
						
						<!-- font*********************************************************************** -->	
						<toolbarbutton sclass="toolbarSeparator" src="/img/separator.bmp"/>	
						<combobox id="fontFamilyCombobox" sclass="toolbarCombobox" width="125px" height="17px" forward="onSelect=onFontFamilySelect">
							<comboitem label="Calibri"  			sclass="calibriFont" forward="onFontFamilySelect(Calibri)"  />
							<comboitem label="Courier New"  		sclass="courierFont" forward="onFontFamilySelect(Courier New)"  />
