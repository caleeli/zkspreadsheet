						<colorbutton id="fontColorBtn" width="20px" height="12px" color="#000000" image="/img/fontColor.gif">
						</colorbutton>
						<colorbutton id="backgroundColorBtn" width="20px" height="12px" color="#FFFFFF" image="/img/backgroundColor.gif">
						</colorbutton>
						
						<!-- border merge******************************************************************* -->
						<toolbarbutton sclass="toolbarSeparator" src="/img/separator.bmp"/>
						<toolbarbutton id="borderBtn" sclass="toolIcon" src="/img/outline.gif"
