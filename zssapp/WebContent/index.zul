<window id="mainWin" apply="org.zkoss.zss.app.MainWindowCtrl" height="100%" width="100%" style="margin:0px;padding:0px">
	<style>
		body{
			padding:0px;
		}
		.z-tabs-scroll .z-tabs-cnt{
			padding-left:0px;
		}
		.z-menubar-hor, .z-menubar-ver {
