						<toolbarbutton id="sortAscending" sclass="toolIcon" src="/img/asc.png">
							<attribute w:name="onClick">
								this.$f('spreadsheet').focus(false);
							</attribute>
						</toolbarbutton>
						<toolbarbutton id="sortDescending" sclass="toolIcon" src="/img/des.png">
							<attribute w:name="onClick">
								this.$f('spreadsheet').focus(false);
							</attribute>
						</toolbarbutton>
					</toolbar>
